{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GRPC Server",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/GrpcServer/bin/Debug/net9.0/GrpcServer.dll",
            "cwd": "${workspaceFolder}/GrpcServer"
        },
        {
            "name": "GRPC Client",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/GrpcClient/bin/Debug/net9.0/GrpcClient.dll",
            "cwd": "${workspaceFolder}/GrpcClient"
        },
        {
            "name": "Debug GrpcServer Tests",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/GrpcServer.Tests/bin/Debug/net7.0/GrpcServer.Tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}/GrpcServer.Tests",
            "console": "internalConsole",
            "stopAtEntry": false
        }
    ]
}